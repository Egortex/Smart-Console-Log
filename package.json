{
	"name": "smart-console-log",
	"displayName": "Smart Console Log",
	"description": "Quick console.log for highlighted variables",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.101.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Smart Console Log",
			"properties": {
				"smartConsoleLog.insertAfterBlock": {
					"type": "boolean",
					"default": true,
					"description": "Insert console.log after block (closing bracket) or just on next line."
				},
				"smartConsoleLog.templates": {
					"type": "object",
					"default": {
						"typescript": "console.log(\"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}:\", ${variable});",
						"javascript": "console.log(\"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}:\", ${variable});",
						"typescriptreact": "console.log(\"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}:\", ${variable});",
						"javascriptreact": "console.log(\"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}:\", ${variable});",
						"python": "print(\"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}:\", ${variable})",
						"php": "echo \"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}: \" . ${variable} . \"\\n\";",
						"java": "System.out.println(\"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}: \" + ${variable});",
						"csharp": "Console.WriteLine($\"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}: {${variable}} \");",
						"ruby": "puts \"ðŸš€ ~ ${className} ~ ${functionName} ~ ${variable}: #{${variable}}\"",
						"go": "fmt.Printf(\"ðŸš€ ~ %s ~ %s ~ %s: %v\\n\", \"${className}\", \"${functionName}\", \"${variable}\", ${variable})",
						"c": "printf(\"ðŸš€ ~ %s ~ %s ~ %s: %d\\n\", \"${className}\", \"${functionName}\", \"${variable}\", ${variable});",
						"cpp": "std::cout << \"ðŸš€ ~ \" << \"${className}\" << \" ~ \" << \"${functionName}\" << \" ~ \" << \"${variable}\" << \": \" << ${variable} << std::endl;"
					},
					"description": "Templates for log statements by language. Supports placeholders: ${className}, ${functionName}, ${variable}."
				}
			}
		},
		"commands": [
			{
				"command": "smartConsoleLog.log",
				"title": "Smart Console Log"
			}
		],
		"keybindings": [
			{
				"command": "smartConsoleLog.log",
				"key": "ctrl+alt+l",
				"when": "editorTextFocus",
				"mac": "cmd+alt+l"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && node esbuild.js",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "node esbuild.js --watch",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run check-types && npm run lint && node esbuild.js --production",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"check-types": "tsc --noEmit",
		"lint": "eslint src",
		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/vscode": "^1.101.0",
		"@types/mocha": "^10.0.10",
		"@types/node": "20.x",
		"@typescript-eslint/eslint-plugin": "^8.31.1",
		"@typescript-eslint/parser": "^8.31.1",
		"eslint": "^9.25.1",
		"esbuild": "^0.25.3",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.8.3",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.5.2"
	}
}
